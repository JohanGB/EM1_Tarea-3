    "# Set the boundary conditions\n",
    "V = np.zeros((ny, nx))\n",
    "V[0,:] = 5\n",
    "V[:,0] = V[:,-1] = V[-1,:] = 0\n",
    "\n",
    "# Calculate the potential using the finite difference method\n",
    "for i in range(10000):\n",
